<%= simple_form_for(@ad) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :image %>
    <%= f.input :url %>
    <%= f.input :impressions %>
    <%= f.input :user_id %>
    
    <%= f.input :views %>
    <%= f.input :live %>
    <%= f.input :clicks %>
    <%= f.input :title %>
    <%= f.input :slug %>
    <%= f.input :website_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
